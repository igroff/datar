#! /usr/bin/env bash
# vim: ft=sh

###############################################################################
###
### Usage : index <file path>
###   adds a document to the index
###############################################################################

MY_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
if [ -z "${UTILS_IMPORTED}" ]; then
  source ${MY_DIR}/utils
fi

echo "running ${BASH_SOURCE[0]} $@"

